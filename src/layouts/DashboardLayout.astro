---
import Navigation from "@/components/Navigation.astro";
import Layout from "./Layout.astro";
const { title } = Astro.props;
---

<Layout title={title}>
    <main class="min-h-dvh h-dvh relative max-h-dvh ">
        <slot />
        <!-- Bottom Navigation -->
        <Navigation transition:name="hero"/>
    </main>
</Layout>